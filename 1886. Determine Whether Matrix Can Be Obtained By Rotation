class Solution {
    public boolean findRotation(int[][] mat, int[][] target) {
        
        for(int i=0; i<4; i++){
            if(Arrays.deepEquals(target, mat)){
                return true;
            }
            rotate(mat);
        }
        return false;
        
    }
    
    void rotate(int[][] arr){
        
        int m = arr.length;
        for(int i=0; i<m; i++){
            for(int j=i; j<m; j++){
                int temp=arr[i][j];
                arr[i][j]=arr[j][i];
                arr[j][i]=temp;
            }
        }
        
        
        for(int i=0; i<m; i++){
            for(int j=0; j<m/2; j++){
                int temp=arr[i][j];
                arr[i][j]=arr[i][m-1-j];
                arr[i][m-1-j]=temp;
            }
        }
        
    }
}
